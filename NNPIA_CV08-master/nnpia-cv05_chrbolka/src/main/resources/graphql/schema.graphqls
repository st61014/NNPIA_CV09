type Query {
    appUser(id: ID!): AppUser!
}

type AppUser {
    id: ID!,
    username: String!,
    active: Boolean!,
    creationDate: String!,
    updateDate: String,
    tasks: [Task!]!
}

type Task {
    id: ID!,
    title: String!,
    description: String!,
    creationDate: String!,
    updateDate: String,
}